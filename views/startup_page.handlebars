<div class="container">

	<ol class="breadcrumb">
	  <li class="active">Home</li>
	</ol>
	
	<div class="col-lg-6">
		{{#if msg}}
			<h3>{{msg}}</h3>
		{{/if}}
		<div class="col-lg-12">
			<div class="panel panel-primary new">
				<div class="panel-heading">
					<h3 class="panel-title">
						New Reports
					</h3>
				</div>
				<div class="panel-body">
					<table class="table table-bordered">
						<tbody>
							<tr class="well">
								<td>New Report</a></td>
								<td><a href="/startup_criteria">Answer Report</a></td>
							</tr>
						</tbody>
					</table>
				</div>
			</div>
		</div>

		<div class="col-lg-12">
			<div class="panel panel-danger new">
				<div class="panel-heading">
					<h3 class="panel-title">
						Pending Reports
					</h3>
				</div>
				<div class="panel-body">
					<table class="table table-bordered">
						<tbody>
							<tr class="well">
								<td>Pending Report</a></td>
								<td><a href="/startup_criteria">Resume Report</a></td>
							</tr>
						</tbody>
					</table>
				</div>
			</div>
		</div>

		<div class="col-lg-12">
			<div class="panel panel-success new">
				<div class="panel-heading">
					<h3 class="panel-title">
						Submitted Reports
					</h3>
				</div>
				<div class="panel-body">
					<table class="table table-bordered">
						<tbody>
							<tr class="well">
								<td>Submitted Report</a></td>
								<td><a href="/startup_criteria">View Report</a></td>
							</tr>
						</tbody>
					</table>
				</div>
			</div>
		</div>
	</div>

	<div class="col-lg-6">
		
		<div class="col-lg-6 col-lg-offset-6">
			<div class="panel panel-default new">
				<div class="panel-heading">
					<h3 class="panel-title">
						Information
					</h3>
				</div>
				<div class="panel-body">
					<table class="table table-bordered">
						<tbody>
							<tr class="well">
								<td><a href="/startup/view_report/:startup_name">View my profile</a></td>
							</tr>
						</tbody>
					</table>
				</div>
			</div>
		</div>
	</div>
</div>
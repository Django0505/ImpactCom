<div class="container panel">
	{{#home_page}}
	<a type="submit" class="col-lg-2 col-lg-offset-8 btn btn-success" href="{{this}}">Home</a>
	{{/home_page}}
	<br>
	<br>
	<div class="list-group panel-body">
		{{#report}}
			<table class="table table-bordered" data-click-to-select="true">
				<thead>
					<tr>
						<th>QUESTIONS</th>
						<th>ANSWERS</th>
					</tr>
				</thead>
				<tbody>
					{{#each Criteria}}
						<tr class="metric_{{@key}}-row">
							<td class="metric_{{@key}}-td">
								{{metric}}
							</td>
							<td class="metric_{{@key}}-td">
								{{value}}
							</td>
						</tr>
					{{/each}}
				</tbody>
			{{/report}}
		</table>
	</div><!-- list -->
</div>
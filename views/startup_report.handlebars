<div class="container">
	<ol class="breadcrumb">
		<li><a href="/startup_page">Home</a></li>
	  <li class="active">Complete Report</li>
	</ol>

	<div class="">
		<div class="col-lg-12">
			<ul class="nav nav-pills nav-justified" role="tab-list">
				<li class="active" role="presentation">
					<a href="#">
						<label>
							Fill in report
						</label>
					</a>
				</li>
				<li role="presentation">
					<a href="#">
						<label>
							Profile
						</label>
					</a>
				</li>
				<li role="presentation">
					<a href="startup_page">
						<label>
							Home
						</label>
					</a>
				</li>
			</ul>
		</div>

		<br/>
		<div class="col-lg-12">
			<form action="/startup_criteria" method="POST">
				<div class="panel panel-default">

					<div class="panel-body">
						<table class="table">
							<thead>	
							</thead>

							{{#create_template}}
								<tbody>
									{{#each Criteria}}
										<tr class="metric_{{@key}}-row">
											<td class="metric_{{@key}}-td">
												{{metric}}
											</td>
											<td class="metric_{{@key}}-td">
												<input type="{{type}}" name="metric_{{@key}}" class="metric_{{@key}}">
											</td>
										</tr>
									{{/each}}

								</tbody>
							{{/create_template}}
						</table>
					</div>
				</div>
				<input name="save_button" type="submit" value="Save Report" class="btn btn-success">
				<input name="submit_button" type="submit" value="Submit Report" class="btn btn-primary col-lg-offset-1">
			</form>
			<br>
		</div>
	</div>
</div>